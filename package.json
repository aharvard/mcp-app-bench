{
  "name": "mcp-app-bench",
  "version": "0.1.0",
  "type": "module",
  "description": "MCP App Bench server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && pnpm build:apps",
    "build:apps": "vite build",
    "start": "node dist/index.js",
    "dev": "concurrently -k -n server,vite -c blue,magenta \"pnpm dev:server\" \"pnpm dev:apps\"",
    "dev:server": "vite-node src/index.ts",
    "dev:apps": "vite build --watch",
    "serve:apps": "vite",
    "inspect:dev": "pnpx @modelcontextprotocol/inspector --config mcp-inspector.config.json --server local-server",
    "inspect:prod": "pnpx @modelcontextprotocol/inspector --config mcp-inspector.config.json --server production-server",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{json,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md,mts}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/ext-apps": "^0.2.2",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "string-width": "^8.1.0",
    "strip-ansi": "^7.1.2",
    "zod": "3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "tailwindcss": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-node": "^1.0.7"
  },
  "packageManager": "pnpm@9.12.3+sha1.4fb96925d434c491285182acf29803bb9aa2e6e5"
}
